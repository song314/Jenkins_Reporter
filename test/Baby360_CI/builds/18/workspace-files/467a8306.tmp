<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- camera module begin-->
    <style name="CompositeSDKSettingItemList">
        <item name="android:orientation">vertical</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_marginBottom">1dp</item>
        <item name="android:divider">#B2323333</item>
        <item name="android:dividerHeight">1px</item>
        <item name="android:cacheColorHint">@android:color/transparent</item>
        <item name="android:choiceMode">singleChoice</item>
        <item name="android:scrollbars">none</item>
    </style>

    <style name="CompositeSDKSettingRow">
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">40dp</item>
    </style>

    <style name="ProgressBarStyle" parent="@android:style/Widget.ProgressBar.Horizontal" />

    <style name="CompositeSDKEffectMenuText">
        <item name="android:textColor">@drawable/composite_sdk_menu_txt</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="CompositeSDKEffectMenuLayout">
        <item name="android:gravity">center</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="CompositeSDKEffectMenuLine">
        <item name="android:background">@color/composite_sdk_effect_menu_txt_bg_selected</item>
        <item name="android:layout_marginTop">2dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:layout_height">2dp</item>
        <item name="android:visibility">gone</item>
    </style>

    <style name="CompositeSDKFullScreenDialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowFullscreen">true</item>
    </style>
    <!-- camera module end-->

    <style name="CompositeSDKPopupDialog" parent="android:Animation">
        <item name="@android:windowEnterAnimation">@anim/composite_sdk_dialog_popup</item>
        <item name="@android:windowExitAnimation">@anim/composite_sdk_dialog_popin</item>
    </style>

    <style name="CompositeSDKThemeDialog" parent="android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:backgroundDimAmount">0.5</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="PinGuoApiDialog" parent="android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:backgroundDimAmount">0.5</item>
    </style>

    <style name="ListItemIcon">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginRight">12dp</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="ListLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">49dp</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="ListItemClickable" parent="@style/ListLayout">
        <item name="android:paddingLeft">14dp</item>
        <!--<item name="android:background">@drawable/bg_transparent_yellow</item>-->
    </style>

    <style name="OptionsLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/option_item_height</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="OptionsItemClickable" parent="@style/OptionsLayout">
        <item name="android:paddingLeft">14dp</item>
        <item name="android:background">@drawable/bg_transparent_yellow</item>
        <item name="android:divider">#003c4248</item>
    </style>

    <style name="OptionsItemIcon">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginRight">12dp</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="OptionsItemText">
        <item name="android:textColor">@drawable/advance_setting_item_text_color</item>
        <item name="android:textSize">@dimen/personal_eamil_text_size</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="OptionsItemSummaryTextStyle">
        <item name="android:textColor">@drawable/advance_setting_item_sub_text_color</item>
        <item name="android:textSize">@dimen/personal_eamil_text_size</item>
        <item name="android:paddingLeft">2dp</item>
        <item name="android:layout_marginTop">4dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="OptionsItemIconMore">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginRight">12dp</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:src">@drawable/advance_setting_item_more_icon</item>
    </style>

    <style name="OptionsScrollView">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="android:fadingEdge">none</item>
    </style>

    <style name="TranslucentStripsWhite">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">1px</item>
        <item name="android:background">#3c4248</item>
    </style>

    <style name="OptionsItemIconMoreNew">
        <item name="android:layout_width">6dp</item>
        <item name="android:layout_height">6dp</item>
        <item name="android:layout_marginRight">12dp</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:background">@drawable/ic_new_tag</item>
    </style>

    <style name="OptionsItemIconMoreNewTag">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginRight">12dp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="OptionClassLayoutStyle" parent="OptionClassLayoutBaseStyle"></style>

    <style name="OptionClassLayoutBaseStyle">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
        <item name="android:background">#c9cdd1</item>
    </style>

    <style name="OptionsItemSelected">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:scaleType">centerCrop</item>
    </style>

    <style name="OptionsLayoutClickable" parent="@style/OptionsLayout">
        <item name="android:paddingLeft">14dp</item>
        <item name="android:clickable">true</item>
        <item name="android:background">@drawable/bg_transparent_yellow</item>
    </style>

    <style name="CheckBoxStyle">
        <item name="android:layout_height">28dp</item>
        <item name="android:layout_width">28dp</item>
        <item name="android:layout_marginRight">20dp</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:button">@null</item>
    </style>

    <style name="OptionsClassTitleStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">14dp</item>
        <item name="android:paddingTop">7dp</item>
        <item name="android:paddingBottom">7dp</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#b8b8b8</item>
        <item name="android:background">#393f44</item>
    </style>

    <style name="TitleLayoutPortLight">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/common_title_width</item>
        <item name="android:background">@color/common_title_bg</item>
        <item name="android:paddingBottom">2px</item>
    </style>
    .
</resources>